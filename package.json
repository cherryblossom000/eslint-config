{
  "name": "@cherryblossom/eslint-config",
  "version": "15.0.2",
  "description": "@cherryblossom000's ESLint configuration.",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "homepage": "https://github.com/cherryblossom000/eslint-config#readme",
  "repository": "github:cherryblossom000/eslint-config",
