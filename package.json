{
  "name": "@cherryblossom/eslint-config",
  "version": "4.0.4",
  "description": "@cherryblossom000's ESLint configuration.",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "homepage": "https://github.com/cherryblossom/eslint-config#readme",
  "repository": "github:cherryblossom000/eslint-config",
  "bugs": "https://github.com/cherryblossom000/eslint-config/issues",
  "license": "MIT",
  "author": "cherryblossom (https://github.com/cherryblossom000)",
  "files": [
    "**/*.js"
  ],
  "engines": {
    "node": "^8.10.0 || ^10.13.0 || >=11.10.1"
  },
  "scripts": {
    "lint": "eslint --max-warnings 0 --report-unused-disable-directives .",
    "lint:dev": "pnpm run lint -- --cache"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@typescript-eslint/parser": "^3.6.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=3.4",
    "eslint": ">=7.3",
    "eslint-plugin-eslint-comments": ">=3.2",
    "eslint-plugin-import": ">=2.21",
    "eslint-plugin-jsdoc": ">=27.1",
    "eslint-plugin-node": ">=11.1",
    "typescript": "*"
  },
  "devDependencies": {
    "@cherryblossom/eslint-config": "file:.",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.3.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^28.6.1",
    "eslint-plugin-node": "^11.1.0",
    "semantic-release": "^17.1.1",
    "typescript": "^3.9.5"
  }
}
