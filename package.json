  "author": "cherryblossom (https://github.com/cherryblossom000)",
  "files": [
    "**/*.js"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint --max-warnings 0 --report-unused-disable-directives .",
