  "license": "MIT",
  "author": "cherryblossom (https://github.com/cherryblossom000)",
  "files": [
    "**/*.js"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint": "eslint --max-warnings 0 .",
    "lint:dev": "dum lint -- --cache"
