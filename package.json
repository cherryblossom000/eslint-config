{
	"name": "@cherryblossom/eslint-config",
	"version": "17.0.0",
	"description": "@cherryblossom000's ESLint configuration.",
	"keywords": [
		"eslint",
		"eslint-config",
		"eslintconfig"
	],
	"homepage": "https://github.com/cherryblossom000/eslint-config#readme",
	"repository": "github:cherryblossom000/eslint-config",
	"bugs": "https://github.com/cherryblossom000/eslint-config/issues",
	"license": "MIT",
	"author": "cherryblossom (https://github.com/cherryblossom000)",
	"files": [
		"**/*.js"
	],
	"engines": {
		"node": ">=14"
	},
	"scripts": {
		"lint": "eslint --max-warnings 0 .",
		"lint:dev": "dum lint -- --cache"
	},
	"publishConfig": {
		"access": "public"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"dependencies": {
		"@typescript-eslint/parser": "^5.52.0"
	},
	"peerDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.25",
		"eslint": "^8.27",
		"eslint-plugin-compat": "^4",
		"eslint-plugin-eslint-comments": "^3.2",
		"eslint-plugin-import": "^2.24",
		"eslint-plugin-jsdoc": ">=37.8 <41",
		"eslint-plugin-node": "^11.1",
		"eslint-plugin-prettier": ">=3 <5",
		"eslint-plugin-unicorn": "^45",
		"prettier": ">=1.13.0",
		"typescript": "^4.1.2"
	},
	"peerDependenciesMeta": {
		"@typescript-eslint/eslint-plugin": {
			"optional": true
		},
		"eslint-plugin-compat": {
			"optional": true
		},
		"eslint-plugin-html": {
			"optional": true
		},
		"eslint-plugin-import": {
			"optional": true
		},
		"eslint-plugin-jsdoc": {
			"optional": true
		},
		"eslint-plugin-markdown": {
			"optional": true
		},
		"eslint-plugin-node": {
			"optional": true
		},
		"typescript": {
			"optional": true
		}
	},
	"devDependencies": {
		"@cherryblossom/eslint-config": "link:",
		"@semantic-release/changelog": "^6.0.2",
		"@semantic-release/git": "^10.0.1",
		"@types/eslint": "^8.21.1",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.34.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-unicorn": "^45.0.2",
		"prettier": "^2.8.4",
		"semantic-release": "^20.1.0",
		"typescript": "^4.9.5"
	}
}
