{
	"name": "@cherryblossom/eslint-config",
	"version": "20.0.1",
	"description": "@cherryblossom000's ESLint configuration.",
	"keywords": [
		"eslint",
		"eslint-config",
		"eslintconfig"
	],
	"homepage": "https://github.com/cherryblossom000/eslint-config#readme",
	"repository": "github:cherryblossom000/eslint-config",
	"bugs": "https://github.com/cherryblossom000/eslint-config/issues",
	"license": "MIT",
	"author": "cherryblossom (https://github.com/cherryblossom000)",
	"files": [
		"**/*.js"
	],
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"lint": "eslint --max-warnings 0 .",
		"lint:dev": "dum lint -- --cache"
	},
	"publishConfig": {
		"access": "public"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1"
	},
	"peerDependencies": {
		"@typescript-eslint/eslint-plugin": "^6",
		"eslint": "^8.38",
		"eslint-plugin-compat": "^4",
		"eslint-plugin-eslint-comments": "^3.2",
		"eslint-plugin-import": "^2.24",
		"eslint-plugin-node": "^11.1",
		"eslint-plugin-prettier": "^5",
		"eslint-plugin-unicorn": ">=47 <49",
		"prettier": ">=3.0.0",
		"typescript": ">=4.1.2 <6"
	},
	"peerDependenciesMeta": {
		"@typescript-eslint/eslint-plugin": {
			"optional": true
		},
		"eslint-plugin-compat": {
			"optional": true
		},
		"eslint-plugin-html": {
			"optional": true
		},
		"eslint-plugin-import": {
			"optional": true
		},
		"eslint-plugin-markdown": {
			"optional": true
		},
		"eslint-plugin-node": {
			"optional": true
		},
		"typescript": {
			"optional": true
		}
	},
	"devDependencies": {
		"@cherryblossom/eslint-config": "link:",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@types/eslint": "^8.44.2",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.46.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-unicorn": "^48.0.1",
		"prettier": "^3.0.1",
		"semantic-release": "^21.0.7",
		"typescript": "~5.1.6"
	}
}
