{
  "name": "@cherryblossom/eslint-config",
  "version": "15.0.0-beta.27",
  "description": "@cherryblossom000's ESLint configuration.",
  "keywords": [
    "eslint",
    "eslint-config",
    "eslintconfig"
  ],
  "homepage": "https://github.com/cherryblossom000/eslint-config#readme",
  "repository": "github:cherryblossom000/eslint-config",
  "bugs": "https://github.com/cherryblossom000/eslint-config/issues",
  "license": "MIT",
  "author": "cherryblossom (https://github.com/cherryblossom000)",
  "files": [
    "**/*.js"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint": "eslint --max-warnings 0 --report-unused-disable-directives .",
    "lint:dev": "pnpm run lint -- --cache"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@typescript-eslint/parser": "^5.8.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5",
    "eslint": "^8.5",
    "eslint-plugin-compat": "^3",
    "eslint-plugin-eslint-comments": "^3.2",
    "eslint-plugin-import": "^2.24",
    "eslint-plugin-jest": "^25",
    "eslint-plugin-jsdoc": ">=34.4 <38",
    "eslint-plugin-node": "^11.1",
    "eslint-plugin-prettier": ">=3 <5",
    "eslint-plugin-promise": "^6",
    "eslint-plugin-unicorn": "^39",
    "prettier": ">=1.13.0",
    "typescript": "^4.1.2"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-compat": {
      "optional": true
    },
    "eslint-plugin-html": {
      "optional": true
    },
    "eslint-plugin-import": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-markdown": {
      "optional": true
    },
    "eslint-plugin-node": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@cherryblossom/eslint-config": "link:",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/eslint": "^8.2.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "prettier": "^2.5.1",
    "semantic-release": "^18.0.1",
    "typescript": "~4.5.4"
  }
}
